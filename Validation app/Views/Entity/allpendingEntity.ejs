<html>
	<head>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
        <link rel="stylesheet" href="/assets/styles/mystyle.css">
        <link rel="stylesheet"
		href="https://cdnjs.cloudflare.com/ajax/libs/malihu-custom-scrollbar-plugin/3.1.  5/jquery.mCustomScrollbar.min.css">
		<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
            integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n"
            crossorigin="anonymous"></script>
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
	</head>
	<body>
		<div class="text-center">
			<h1 class="header">All Pending Entity</h1>
        </div>
        
		<div class="text-center">
            <a href="/entity/allPendingEntity" class="btn btn-primary">All Pending Entity</a>
            <a href="/logout" class="btn btn-danger">Logout</a>
        </div>
        
        <div class="center">
            <h3 class="text">All Pending Entity's</h3>
            <table class="table table-striped">
                <thead>
                    <td>ID</td>
                    <td>Name</td>
                    <td>User Name</td>
                    <td>Email</td>
                    <td>Contact No</td>
                    <td>Address</td>
                    <td>Vote</td>
                    <td>Action</td>
                </thead>
                <tbody id="tbody">
                    <% 
                        entities.forEach(function(std){ %>
                            <tr>
                                <td><%= std.id %></td>
                                <td><%= std.name %></td>
                                <td><%= std.username %></td>
                                <td><%= std.email %></td>
                                <td><%= std.phone %></td>
                                <td><%= std.address %></td>
                                <td><%= std.vote %></td>
                                <td>
                                    <a href="/entity/updateEntity/<%= std.id %>" class="btn btn-warning">View</a>
                                </td>
                            </tr>
        
                    <%
                        }); 
                    %>
                </tbody>
            </table>
        </div>
	</body>
</html>